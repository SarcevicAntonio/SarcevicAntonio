    "daisyui": "^2.15.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fenceparser": "^2.0.0",
    "github-slugger": "^1.4.0",
